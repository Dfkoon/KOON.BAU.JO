    let sid = sessionStorage.getItem('v_session_id');\n    if (!sid) {\n        sid = 's_' + Math.random().toString(36).substring(2, 15);\n        sessionStorage.setItem('v_session_id', sid);\n    }\n    return sid;\n};\n\nconst AnalyticsTracker = ({ children }) => {\n    const location = useLocation();\n    const entryTime = useRef(Date.now());\n    const prevPath = useRef(location.pathname);\n\n    useEffect(() => {\n        const sessionId = getSessionId();\n        const visitorId = getVisitorId();\n\n        // Function to log the time spent on the PREVIOUS page\n        const logDuration = () => {\n            const now = Date.now();\n            const duration = Math.round((now - entryTime.current) / 1000);\n\n            // Capping: if user leaves tab open for hours, we cap at 30 mins\n            const cappedDuration = Math.min(duration, 1800);\n\n            // Filters: Only log if stay is at least 2 seconds (avoid accidental clicks/bots)\n            if (duration >= 2) {\n                analyticsService.logPageView(prevPath.current, cappedDuration, sessionId, visitorId);\n            }\n        };\n\n        // On path change\n        logDuration();\n\n        // Update markers for the NEW page\n        entryTime.current = Date.now();\n        prevPath.current = location.pathname;\n\n        // Cleanup: also log when the component unmounts (rare for this wrapper)\n        return () => {\n            // No-op here usually because next useEffect handles it, \n            // but we can add window unload listener\n        };\n    }, [location.pathname]);\n\n    // Handle tab closing / refresh\n    useEffect(() => {\n        const handleUnload = () => {\n            const sessionId = getSessionId();\n            const visitorId = getVisitorId();\n            const duration = Math.round((Date.now() - entryTime.current) / 1000);\n            const cappedDuration = Math.min(duration, 1800);\n\n            if (duration >= 2) {\n                analyticsService.logPageView(location.pathname, cappedDuration, sessionId, visitorId);\n            }\n        };\n\n        window.addEventListener('beforeunload', handleUnload);\n        return () => window.removeEventListener('beforeunload', handleUnload);\n    }, [location.pathname]);\n\n    return <>{children}</>;\n};\n\nexport default AnalyticsTracker;\n","usedDeprecatedRules":[]},{"filePath":"/Users/hussienaldayyat/Desktop/koon.bau.jo/src/components/AnimatedHero.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"/Users/hussienaldayyat/Desktop/koon.bau.jo/src/components/AnimatedLogo.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"/Users/hussienaldayyat/Desktop/koon.bau.jo/src/components/BackgroundPattern.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"/Users/hussienaldayyat/Desktop/koon.bau.jo/src/components/Ballpit.jsx","messages":[],"suppressedMessages":[{"ruleId":"react-hooks/exhaustive-deps","severity":1,"message":"React Hook useEffect has missing dependencies: 'followCursor' and 'props'. Either include them or remove the dependency array.","line":824,"column":8,"nodeType":"ArrayExpression","endLine":824,"endColumn":10,"suggestions":[{"desc":"Update the dependencies array to be: [followCursor, props]","fix":{"range":[28674,28676],"text":"[followCursor, props]"}}],"suppressions":[{"kind":"directive","justification":""}]}],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"/Users/hussienaldayyat/Desktop/koon.bau.jo/src/components/CleanTestimonials.jsx","messages":[{"ruleId":"no-unused-vars","severity":2,"me