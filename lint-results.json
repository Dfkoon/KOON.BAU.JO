Questions] = useState(new Set());\n    const [currentQuestionIndex, setCurrentQuestionIndex] = useState(0);\n\n    // New state for reporting\n    const [reportDialogOpen, setReportDialogOpen] = useState(false);\n    const [questionToReport, setQuestionToReport] = useState(null);\n    const [questionToReportIndex, setQuestionToReportIndex] = useState(null);\n\n    if (!quiz) return null;\n\n    const effectiveDir = quiz.direction === 'ltr' ? 'ltr' : (isRtl ? 'rtl' : 'ltr');\n    const isQuizLtr = effectiveDir === 'ltr';\n\n    const handleOptionChange = (questionId, value) => {\n        if (isReviewMode) return;\n        setSelectedAnswers(prev => ({\n            ...prev,\n            [questionId]: value\n        }));\n    };\n\n    const handleFlagQuestion = (question, index) => {\n        setQuestionToReport(question);\n        setQuestionToReportIndex(index);\n        setReportDialogOpen(true);\n\n        // Also keep the local flagging visual\n        const newFlagged = new Set(flaggedQuestions);\n        if (newFlagged.has(question.id)) {\n            newFlagged.delete(question.id);\n        } else {\n            newFlagged.add(question.id);\n        }\n        setFlaggedQuestions(newFlagged);\n    };\n\n    const handleNavigateToQuestion = (index) => {\n        setCurrentQuestionIndex(index);\n        // Scroll to question\n        setTimeout(() => {\n            const element = document.getElementById(`q-${index}`);\n            if (element) {\n                element.scrollIntoView({ behavior: 'smooth', block: 'start' });\n            }\n        }, 100);\n    };\n\n    const handleFinish = () => {\n        setFinishTime(new Date());\n        setIsReviewMode(true);\n    };\n\n    const handleRestart = () => {\n        setSelectedAnswers({});\n        setIsReviewMode(false);\n        setFinishTime(null);\n        setFlaggedQuestions(new Set());\n        setCurrentQuestionIndex(0);\n        // Reset start time conceptually, though in this simple version we just re-render\n    };\n\n    const calculateScore = () => {\n        let score = 0;\n        quiz.questions.forEach(q => {\n            if (selectedAnswers[q.id] === q.correctAnswer) {\n                score++;\n            }\n        });\n        return score;\n    };\n\n    const score = calculateScore();\n    const totalQuestions = quiz.questions.length;\n    const grade = ((score / totalQuestions) * 10).toFixed(2); // Grade out of 10\n\n    // Moodle Colors\n    const colors = {\n        infoBoxBg: '#f8f9fa',\n        infoBoxBorder: '#dee2e6',\n        questionBg: '#e7f3f5',\n        feedbackBg: '#fff3cd',\n        feedbackText: '#856404',\n        correct: '#d4edda', // Light green for correct option bg\n        incorrect: '#f8d7da', // Light red for incorrect option bg\n    };\n\n    return (\n        <Dialog\n            fullScreen\n            open={open}\n            onClose={onClose}\n            sx={{ direction: effectiveDir }}\n        >\n            <AppBar sx={{ position: 'relative', bgcolor: '#333' }}>\n                <Toolbar sx={{ flexDirection: isQuizLtr ? 'row' : 'row-reverse' }}>\n                    <IconButton\n                        edge={isQuizLtr ? \"start\" : \"end\"}\n                        color=\"inherit\"\n                        onClick={onClose}\n                        aria-label=\"close\"\n                    >\n                        <Close />\n                    </IconButton>\n                    <Box sx={{ mx: 2, flex: 1, textAlign: isQuizLtr ? 'left' : 'right' }}>\n                        <Typography variant=\"h6\" component=\"div\">\n                            {typeof quiz.title === 'object' ? (isQuizLtr ? (quiz.title.en || quiz.title.ar) : (quiz.title.ar || quiz.title.en)) : quiz.title}\n                        </Typography>\n                        {!isReviewMode && (\n                            <Typography variant=\"caption\" sx={{ color: '#bbb', display: 'block', mt: 0.5 }}>\n                                {isQuizLtr\n